| /* Define dbus_watch_get_unix_fd to an innocuous variant, in case <limits.h> declares dbus_watch_get_unix_fd.
|    For example, HP-UX 11i <limits.h> declares gettimeofday.  */
| #define dbus_watch_get_unix_fd innocuous_dbus_watch_get_unix_fd
| 
| /* System header to define __stub macros and hopefully few prototypes,
|     which can conflict with char dbus_watch_get_unix_fd (); below.
|     Prefer <limits.h> to <assert.h> if __STDC__ is defined, since
|     <limits.h> exists even on freestanding compilers.  */
| 
| #ifdef __STDC__
| # include <limits.h>
| #else
| # include <assert.h>
| #endif
| 
| #undef dbus_watch_get_unix_fd
| 
| /* Override any GCC internal prototype to avoid an error.
|    Use char because int might match the return type of a GCC
|    builtin and then its argument prototype would still apply.  */
| #ifdef __cplusplus
| extern "C"
| #endif
| char dbus_watch_get_unix_fd ();
| /* The GNU C library defines this for functions which it implements
|     to always fail with ENOSYS.  Some functions are actually named
|     something starting with __ and the normal name is an alias.  */
| #if defined __stub_dbus_watch_get_unix_fd || defined __stub___dbus_watch_get_unix_fd
| choke me
| #endif
| 
| int
| main ()
| {
| return dbus_watch_get_unix_fd ();
|   ;
|   return 0;
| }
